    "def fizzbuzz(n):\n",
    "    for i in range(1, n+1):\n",
    "        if i % 3 == 0 and i % 5 == 0:\n",
    "            print(\"FizzBuzz\")\n",
    "\n",
    "        elif i % 3 == 0:\n",
    "            print(\"Fizz\")\n",
    "\n",
    "        elif i % 5 == 0:\n",
    "            print(\"Buzz\")\n",
    "\n",
    "        else:\n",
    "            print(i)\n",
    "\n",
    "fizzbuzz(20)\n",
    "\n"
   ]
